## Profiles 
  /etc/csh.cshrc                       -> $(SEC_CONFIG) ;
  /etc/csh.login                       -> $(SEC_CONFIG) ;
  /etc/profile                         -> $(SEC_CONFIG) ;
  /etc/profile.d                       -> $(SEC_CONFIG) (recurse = 1);
